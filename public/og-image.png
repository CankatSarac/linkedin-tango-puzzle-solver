<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="630" viewBox="0 0 1200 630">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563eb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7c3aed;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="600" y="200" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="64" font-weight="bold">
    Tango Puzzle Solver
  </text>
  
  <!-- Subtitle -->
  <text x="600" y="260" text-anchor="middle" fill="#e5e7eb" font-family="Arial, sans-serif" font-size="32">
    Free Online LinkedIn Tango Game Solver
  </text>
  
  <!-- Game grid representation -->
  <g transform="translate(450, 320)">
    <!-- Grid cells -->
    <rect x="0" y="0" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="80" y="0" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="160" y="0" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="240" y="0" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    
    <rect x="0" y="80" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="80" y="80" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="160" y="80" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    <rect x="240" y="80" width="60" height="60" fill="white" stroke="#333" stroke-width="2" rx="8"/>
    
    <!-- Sun symbols -->
    <circle cx="30" cy="30" r="20" fill="#FFD700"/>
    <circle cx="190" cy="30" r="20" fill="#FFD700"/>
    <circle cx="110" cy="110" r="20" fill="#FFD700"/>
    <circle cx="270" cy="110" r="20" fill="#FFD700"/>
    
    <!-- Moon symbols -->
    <circle cx="110" cy="30" r="20" fill="#4169E1"/>
    <circle cx="270" cy="30" r="20" fill="#4169E1"/>
    <circle cx="30" cy="110" r="20" fill="#4169E1"/>
    <circle cx="190" cy="110" r="20" fill="#4169E1"/>
    
    <!-- Constraint symbols -->
    <text x="65" y="40" text-anchor="middle" fill="#2563eb" font-family="Arial, sans-serif" font-size="24" font-weight="bold">=</text>
    <text x="225" y="40" text-anchor="middle" fill="#dc2626" font-family="Arial, sans-serif" font-size="24" font-weight="bold">≠</text>
  </g>
  
  <!-- Call to action -->
  <text x="600" y="550" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="28">
    Solve puzzles with suns ☀️ and moons 🌙!
  </text>
</svg>
